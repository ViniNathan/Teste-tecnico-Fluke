{
	"name": "backend",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"start": "npm run db:up && tsx src/api/server.ts",
		"start:worker": "tsx src/worker/runner.ts",
		"dev": "tsx watch src/api/server.ts",
		"worker": "tsx src/worker/runner.ts",
		"db:create": "node-pg-migrate --migrations-dir src/db/migrations create",
		"db:up": "node-pg-migrate --migrations-dir src/db/migrations up",
		"db:down": "node-pg-migrate --migrations-dir src/db/migrations down",
		"db:redo": "npm run db:down && npm run db:up",
		"db:status": "node-pg-migrate --migrations-dir src/db/migrations status",
		"test": "cross-env NODE_ENV=test vitest run",
		"test:watch": "cross-env NODE_ENV=test vitest watch",
		"lint": "biome check .",
		"lint:fix": "biome check . --write"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"@types/express": "^5.0.6",
		"@types/node": "^25.0.10",
		"cors": "^2.8.6",
		"dotenv": "^17.2.3",
		"express": "^5.2.1",
		"json-logic-js": "^2.0.5",
		"node-pg-migrate": "^8.0.4",
		"pg": "^8.17.2",
		"pino": "^10.3.0",
		"swagger-jsdoc": "^6.2.8",
		"swagger-ui-express": "^5.0.1",
		"tsx": "^4.21.0",
		"typescript": "^5.9.3",
		"ws": "^8.19.0",
		"zod": "^4.3.6"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.13",
		"@types/cors": "^2.8.19",
		"@types/pg": "^8.16.0",
		"@types/supertest": "^6.0.3",
		"@types/swagger-jsdoc": "^6.0.4",
		"@types/swagger-ui-express": "^4.1.8",
		"@types/ws": "^8.18.1",
		"cross-env": "^10.1.0",
		"pino-pretty": "^13.1.3",
		"supertest": "^7.2.2",
		"vitest": "^4.0.18"
	}
}